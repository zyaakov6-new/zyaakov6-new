# Database: SQLite is configured in prisma/schema.prisma (no DATABASE_URL needed)

# JWT secret for signing auth tokens (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET="change-me-to-a-random-64-character-hex-string"

# Encryption key for encrypting provider credentials at rest (32 bytes hex = 64 chars)
ENCRYPTION_KEY="change-me-to-a-random-64-character-hex-string"

# App URL (used for CORS, redirects, etc.)
NEXT_PUBLIC_APP_URL="http://localhost:3000"
